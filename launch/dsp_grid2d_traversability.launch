<?xml version="1.0"?>
<launch>
	<node name="dsp" pkg="dsp" type="dsp_node" ns="husky" output="screen">
		<param name="use_gazebo_odom" type="bool" value="true"/>
		<param name="odom_topic" type="String" value="odometry/imu"/>
		<param name="odom_frame_id" type="String" value="map"/>
		<param name="base_link_frame" type="String" value="base_link"/>
		<param name="map_topic" type="String" value="occupancy_map_local"/>
		<param name="use_3d" type="bool" value="false"/>
		<param name="spline_step_" type="double" value="0.25"/>
		<param name="update_rate" type="double" value="10.0"/>
	</node>
</launch>
